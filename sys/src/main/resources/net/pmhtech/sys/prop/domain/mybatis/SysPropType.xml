<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper	PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="SysPropType">

	<insert id="insert" parameterType="sysPropType">
	/*	SysPropType.insert	*/
	
	INSERT INTO SYS_PROP_TYPE(
		,XTYPE_NM
		,CLAZZ_NM
		,CHANGE_FUNC
		,PROPS
		,CREATER
		,CREATE_DT
		,CHANGER
		,CHANGE_DT
		,CHANGE_IP
	
	)VALUES(
		#{XTYPE_NM}
		,#{CLAZZ_NM}
		,#{CHANGE_FUNC}
		,#{PROPS}
		,#{CREATER}
		,#{CREATE_DT}
		,#{CHANGER}
		,#{CHANGE_DT}
		,#{CHANGE_IP}
	)
	
	
	</insert>
	
	<update id="update" parameterType="sysPropType">
	/*	SysPropType.update	*/
	UPDATE  SYS_PROP_TYPE
	SET		CLAZZ_NM	=#{CLAZZ_NM}
			,CHANGE_FUNC=#{CHANGE_FUNC}
			,PROPS		=#{PROPS}
			,CREATER	=#{CREATER}
			,CREATE_DT	=#{CREATE_DT}
			,CHANGER	=#{CHANGER}
			,CHANGE_DT	=#{CHANGE_DT}
			,CHANGE_IP	=#{CHANGE_IP}
	WHERE	XTYPE_NM	=#{XTYPE_NM}
	</update>
	
	<delete id="delete" parameterType="paramMap">
	/*	SysPropType.delete	*/
	DELETE	FROM SYS_PROP_TYPE
	WHERE	XTYPE_NM		= #{XTYPE_NM}
	</delete>

	<select id="selectList" parameterType="paramMap" resultType="resultMap">
	/*	SysPropType.selectList	*/
	SELECT	,A.XTYPE_NM
			,A.CLAZZ_NM
			,A.CHANGE_FUNC
			,A.PROPS
			,A.CREATER
			,A.CREATE_DT
			,A.CHANGER
			,A.CHANGE_DT
			,A.CHANGE_IP
	FROM	SYS_PROP_TYPE A
	</select>
</mapper> 
