<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper	PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="LogMst">

	<insert id="insert" parameterType="logMst">
	/*	LogMst.insert	*/
	
	INSERT INTO LOG_MST(
		PGM_ID
		,LOG_TIME
		,PLF_GBN
		,PLF_USER_ID
		,TITLE
		,SNAPSHOT
		,VIEW_CNT
		,CREATER
		,CREATE_DT
		,CREATE_IP
	)VALUES(
		#{PGM_ID}
		,#{LOG_TIME}
		,#{PLF_GBN}
		,#{PLF_USER_ID}
		,#{TITLE}
		,#{SNAPSHOT}
		,#{VIEW_CNT}
		,#{CREATER}
		,#{CREATE_DT}
		,#{CREATE_IP}
	)
	</insert>
</mapper> 
